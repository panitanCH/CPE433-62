{
    "DocumentRoot": ".",
    "Port": "8080",
	"ThreadPoolSize" : "10",                                 
    "AllowThreadModel" : "Pool|Multi|Single",                          //thread model selection
    "ThreadModel" : "Single",                                          //Chahge Thread Model Here
    "Plugins" : [
        {
            "Path" : "stat",
            "Class" : "DNWS.StatPlugin",
            "Preprocessing" : "true",
            "Postprocessing" : "false" ,
            "Singleton" : "false"
        },
        {
            "Path" : "ox",
            "Class" : "DNWS.OXPlugin",
            "Preprocessing" : "false",
            "Postprocessing" :"false" 
        },
        {
            "Path" : "delay",
            "Class" : "DNWS.DelayPlugin",
            "Preprocessing" : "false",
            "Postprocessing" :"false" 
        },
		{
            "Path" : "clientinfo",
            "Class" : "DNWS.ClientInfoPlugin",
            "Preprocessing" : "false",
            "Postprocessing" :"false" 
        }
    ]
}
